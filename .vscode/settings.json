{
  "eslint.workingDirectories": ["./packages/*/"],
  "editor.tabSize": 2,
  "eslint.validate": [
      {
          "language": "vue",
          "autoFix": false
      },
      {
          "language":"html",
          "autoFix": false
      },
      {
          "language":"javascript",
          "autoFix": false
      },
      {
          "language":"typescript",
          "autoFix": false
      }
  ],
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "liveshare.populateGitCoAuthors": "never",
  "files.trimTrailingWhitespace": true,
  "cSpell.words": [
    "docker",
    "macio",
    "musikstreaming",
    "simplifizieren",
    "tailwind",
    "vetur",
    "visual"
]
}
