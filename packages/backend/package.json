    "@types/serve-favicon": "2.5.2",
    "@types/spotify-web-api-node": "4.0.2",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "axios": "0.21.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
